{
  "manifest_version": 3,
  "name": "email.love - Anonymous Email Forwarding",
  "short_name": "email.love",
  "author": "loveuemail",
  "description": "Open-source Anonymous Email Forwarding, create unlimited email aliases for free",
  "homepage_url": "https://email.love",
  "version": "2.3.8",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "96": "img/icon_96.png",
    "128": "img/icon_128.png"
  },
  "action": {
    "default_title": "email.love",
    "default_popup": "app.html"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "browser-extension@loveuemail"
    }
  }
}